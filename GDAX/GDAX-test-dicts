orderDict = {}


with open('matrix_CSV.csv') as matrixCSV:
    readMatrix = csv.reader(matrixCSV, delimiter=',')

    for row in readMatrix:
        print(row)
        volume = row[0]
        peg = row[1]
        tstate = row[2]
        oID = row[3]

        orderDict['size'] = volume
        orderDict['price'] = peg
        orderDict['side'] = tstate
        orderDict['product_id'] = oID

print(orderDict)
